
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<title>W3.CSS</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<body>

    <!-- Sidebar -->
    <div class="w3-sidebar w3-light-grey w3-bar-block" style="width:25%">
        <h3 class="w3-bar-item">Menu</h3>
        <a href="#" class="w3-bar-item w3-button">Ürünler</a>
        <a href="#" class="w3-bar-item w3-button">Müşteriler</a>
        <a href="#" class="w3-bar-item w3-button">Kategoriler</a>
        <a href="#" class="w3-bar-item w3-button">Yardım</a>
        <a href="#" class="w3-bar-item w3-button">Çıkış Yap</a>
    </div>

    <!-- Page Content -->
    <div style="margin-left:25%">

        <div class="w3-container w3-teal">
            <h1>My Page</h1>
        </div>

        

        <div class="w3-container">
          @RenderBody()
        </div>

    </div>

</body>
</html>

